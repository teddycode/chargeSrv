                },
                "oper_name": {
                    "type": "string",
                    "example": "施肥"
                },
                "start_time": {
                    "type": "string",
                    "example": "1594382265"
                }
            }
        },
        "schema.FarmSwag": {
            "type": "object",
            "properties": {
                "end_time": {
                    "type": "string",
                    "example": "1595382265"
                },
                "point": {
                    "type": "string",
                    "example": "point001"
                },
                "star_time": {
                    "type": "string",
                    "example": "1594382265"
                }
            }
        },
        "schema.PasswordSwag": {
            "type": "object",
            "properties": {
                "new_password": {
                    "description": "新密码",
                    "type": "string",
                    "example": "123456"
                },
                "old_password": {
                    "description": "旧密码",
                    "type": "string",
                    "example": "1234"
                }
            }
        },
        "schema.PicSwag": {
            "type": "object",
            "properties": {
                "end_time": {
                    "type": "string",
                    "example": "1595382265"
                },
                "point": {
                    "type": "string",
                    "example": "point001"
                },
                "star_time": {
                    "type": "string",
                    "example": "1594382265"
                }
            }
        },
        "schema.QueryTransNumSwag": {
            "type": "object",
            "required": [
                "type"
            ],
            "properties": {
                "type": {
                    "type": "integer"
                }
            }
        },
        "schema.RegSwag": {
            "type": "object",
            "required": [
                "identity",
                "password",
                "username"
            ],
            "properties": {
                "identity": {
                    "description": "identity",
                    "type": "string",
                    "example": "4522261111111111"
                },
                "password": {
                    "description": "密码",
                    "type": "string",
                    "example": "1234"
                },
                "username": {
                    "description": "用户名",
                    "type": "string",
                    "example": "teddy"
                }
            }
        },
        "schema.RevokeSwag": {
            "type": "object",
            "properties": {
                "identity": {
                    "type": "string",
                    "example": "4522261111111111"
                },
                "user_name": {
                    "type": "string",
                    "example": "teddy"
                }
            }
        },
        "schema.SensorSwag": {
            "type": "object",
            "properties": {
                "end_time": {
                    "type": "string",
                    "example": "1595382265"
                },
                "point": {
                    "type": "string",
                    "example": "point001"
                },
                "star_time": {
                    "type": "string",
                    "example": "1594382265"
                }
            }
        },
        "schema.UploadSwag": {
            "type": "object",
            "properties": {
                "point": {
                    "type": "string",
                    "example": "point001"
                },
                "raw": {
                    "type": "string",
                    "example": "{\"point\":\"point001\",\"type\":\"temperature\",\"point\":\"26.2\",\"unit\":\"C\"}"
                },
                "type": {
                    "type": "string",
                    "example": "p"
                }
            }
        },
        "schema.VerifySwag": {
            "type": "object",
            "properties": {
                "hash": {
                    "type": "string",
                    "example": "45d44ca55d"
                }
            }
        }
    },
